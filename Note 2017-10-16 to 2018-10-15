z=[42.25,41.53,40.38,32.98,39.23,43.63,45.48]
zz=[40.33,34.02,41.34,34.04,36.77,47.67,44.90]
aa=np.array(x)
bb=np.array(y)
cc=np.array(z)
dd=np.array(zz)
print aa.sum()+bb.sum()+cc.sum()+dd.sum()
